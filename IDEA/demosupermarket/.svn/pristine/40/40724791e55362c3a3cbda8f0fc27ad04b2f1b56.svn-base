package com.badboyjy.bean;

public class Supplier {
    private Integer supplierId;//供应商编号
    private String supplierName;//供应商名称
    private String supplierDesc;//供应商描述
    private String supplierPerName;//供应商联系人
    private String supplierPhone;//供应商电话
    private String supplierChuanZhen;//供应商传真
    private String supplierAddress;//供应商地址

    @Override
    public String toString() {
        return "Supplier{" +
                "supplierId=" + supplierId +
                ", supplierName='" + supplierName + '\'' +
                ", supplierDesc='" + supplierDesc + '\'' +
                ", supplierPerName='" + supplierPerName + '\'' +
                ", supplierPhone='" + supplierPhone + '\'' +
                ", supplierChuanZhen='" + supplierChuanZhen + '\'' +
                ", supplierAddress='" + supplierAddress + '\'' +
                '}';
    }

    public Integer getSupplierId() {
        return supplierId;
    }

    public void setSupplierId(Integer supplierId) {
        this.supplierId = supplierId;
    }

    public String getSupplierName() {
        return supplierName;
    }

    public void setSupplierName(String supplierName) {
        this.supplierName = supplierName;
    }

    public String getSupplierDesc() {
        return supplierDesc;
    }

    public void setSupplierDesc(String supplierDesc) {
        this.supplierDesc = supplierDesc;
    }

    public String getSupplierPerName() {
        return supplierPerName;
    }

    public void setSupplierPerName(String supplierPerName) {
        this.supplierPerName = supplierPerName;
    }

    public String getSupplierPhone() {
        return supplierPhone;
    }

    public void setSupplierPhone(String supplierPhone) {
        this.supplierPhone = supplierPhone;
    }

    public String getSupplierChuanZhen() {
        return supplierChuanZhen;
    }

    public void setSupplierChuanZhen(String supplierChuanZhen) {
        this.supplierChuanZhen = supplierChuanZhen;
    }

    public String getSupplierAddress() {
        return supplierAddress;
    }

    public void setSupplierAddress(String supplierAddress) {
        this.supplierAddress = supplierAddress;
    }

    public Supplier() {
    }

    public Supplier(Integer supplierId, String supplierName, String supplierDesc, String supplierPerName, String supplierPhone, String supplierChuanZhen, String supplierAddress) {
        this.supplierId = supplierId;
        this.supplierName = supplierName;
        this.supplierDesc = supplierDesc;
        this.supplierPerName = supplierPerName;
        this.supplierPhone = supplierPhone;
        this.supplierChuanZhen = supplierChuanZhen;
        this.supplierAddress = supplierAddress;
    }
}
